{
  "name": "cloud-function-boilerplate",
  "version": "1.0.0",
  "main": "function.js",
  "private": true,
  "scripts": {
	"start": "node server.js",
    "local": "./scripts/local.sh",
    "fire": "./scripts/fire.sh",
    "deploy": "./scripts/deploy-all.sh",
    "perms": "chmod +x ./scripts/*.sh",
    "prune": "rm -rf tmp/*",
    "test": "node --no-warnings --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "type": "module",
  "keywords": [
    "cloud function",
    "template",
    "serverless"
  ],
  "author": "ak@mixpanel.com",
  "license": "ISC",
  "description": "AK's cloud function boilerplate",
  "dependencies": {
    "@google-cloud/functions-framework": "^3.4.4",
    "@google-cloud/logging-winston": "^6.0.0",
    "@google-cloud/storage": "^7.12.0",
    "ak-fetch": "^1.0.892",
    "ak-tools": "^1.0.696911",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "socket.io": "^4.8.1",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.1.4"
  }
}
